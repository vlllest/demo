#include <iostream>
using namespace std;

int main() 
{
	setlocale(0, "");
	int number;
	cout << "Введите число: ";
	cin >> number;

	int square = number * number;
	cout << "Квадрат числа: " << square << endl;

	int cube = number * number * number;
	cout << "Куб числа: " << cube << endl;

	return 0;
}
